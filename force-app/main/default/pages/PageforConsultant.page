<apex:page standardController="Consultant__c">
   <apex:pageBlock title="Consultant Summary">
       <apex:form >
           <apex:pageBlockSection title="Consultant Information">
            <apex:inputField value="{! Consultant__c.Name }"/>
               <apex:inputField value="{!Consultant__c.FirstName__c }"/>
               <apex:inputField value="{!Consultant__c.LastName__c }"/>
               <apex:inputField value="{!Consultant__c.DOB__c }"/>
                <apex:inputField value="{! Consultant__c.Sex__c }"/>
               <apex:inputField value="{! Consultant__c.Email__c }"/>
               <apex:inputField value="{! Consultant__c.Married__c}"/>
               <apex:inputField value="{! Consultant__c.Education__c}"/>
        </apex:pageBlockSection>
           
        
        <apex:commandButton action="{! save }" value="Save" />
    </apex:form>
        
      
    </apex:pageBlock>
</apex:page>